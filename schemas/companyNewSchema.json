{
  "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
      "type": "object",
        "title": "The root schema",
          "description": "The root schema comprises the entire JSON document.",
            "default": { },
  "examples": [
    {
      "handle": "harmony",
      "name": "Harmony",
      "num_employees": 45,
      "description": "Natural food coop",
      "logo_url": "https://via.placeholder.com/200"
    }
  ],
    "required": [
      "handle",
      "name",
      "num_employees",
      "description",
      "logo_url"
    ],
      "properties": {
    "handle": {
      "$id": "#/properties/handle",
        "type": "string",
          "title": "The handle schema",
            "description": "An explanation about the purpose of this instance.",
              "default": "",
                "examples": [
                  "harmony"
                ]
    },
    "name": {
      "$id": "#/properties/name",
        "type": "string",
          "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
              "default": "",
                "examples": [
                  "Harmony"
                ]
    },
    "num_employees": {
      "$id": "#/properties/num_employees",
        "type": "integer",
          "title": "The num_employees schema",
            "description": "An explanation about the purpose of this instance.",
              "default": 0,
                "examples": [
                  45
                ]
    },
    "description": {
      "$id": "#/properties/description",
        "type": "string",
          "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
              "default": "",
                "examples": [
                  "Natural food coop"
                ]
    },
    "logo_url": {
      "$id": "#/properties/logo_url",
        "type": "string",
          "title": "The logo_url schema",
            "description": "An explanation about the purpose of this instance.",
              "default": "",
                "examples": [
                  "https://via.placeholder.com/200"
                ]
    }
  },
  "additionalProperties": true
}