{
  "$id": "http://example.com/example.json",
  "type": "object",
  "addionalProperties": false,
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": ["Harmony Foods"]
    },
    "handle": {
      "$id": "/properties/handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "examples": ["harmony"]
    },
    "logo": {
      "$id": "/properties/logo",
      "type": "string",
      "title": "The Logo Schema ",
      "default": "",
      "format": "uri",
      "examples": [
        "https://via.placeholder.com/200"
      ]
    },
    "_token": {
      "$id": "/properties/token",
      "type": "string",
      "title": "The Token Schema ",
      "default": "",
      "examples": [
        "blah123blah"
      ]
    }
  },
  "required": ["name", "handle"]
}